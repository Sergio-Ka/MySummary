.contact
  min-height 24.857rem
  &__table
    margin 1.429rem 0 0 0
    display table
  &__row
    display table-row
    height 4rem
  &__cell
    display table-cell
    font-size 1.53rem
    &-mail
      text-decoration none
      color #000000
    &-telephone
      text-decoration none
      color #000000
    &-site
      text-decoration none
      color #000000
  &__cell_color_blue
    padding-right 3.1rem
    color #3d6cb0
    font-weight bold
  &__cell_case_upper
    text-transform uppercase
  &__cell_word-break_normal
    word-break normal